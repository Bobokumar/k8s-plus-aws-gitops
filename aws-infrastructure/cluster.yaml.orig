apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig

metadata:
  name: cluster
  region: AWS_DEFAULT_REGION
  version: "1.14"

vpc:
  subnets:
    private:
      AWS_DEFAULT_REGIONa:
        id: "PrivateSubnet1ID"
      AWS_DEFAULT_REGIONb:
        id: "PrivateSubnet2ID"
    public:
      AWS_DEFAULT_REGIONa:
        id: "PublicSubnet1ID"
      AWS_DEFAULT_REGIONb:
        id: "PublicSubnet2ID"

cloudWatch:
  clusterLogging:
    enableTypes:
      - "audit"
      - "authenticator"
      - "api"
      - "controllerManager"
      - "scheduler"

fargateProfiles:
  - name: default
    selectors:
      - namespace: default
      - namespace: kube-system
  - name: ghost
    selectors:
      - namespace: ghost